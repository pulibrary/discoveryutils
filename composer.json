{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "guzzlehttp/guzzle": "^6.3@dev",
        "symfony/apache-pack": "*",
        "symfony/asset": "4.3.*",
        "symfony/browser-kit": "4.3.*",
        "symfony/console": "4.3.*",
        "symfony/css-selector": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "4.3.*",
        "symfony/monolog-bundle": "^3.3",
        "symfony/twig-bundle": "4.3.*",
        "symfony/yaml": "4.3.*"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/php-invoker": "1.1.x-dev",
        "symfony/phpunit-bridge": "^5.1@dev"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "psr-0": {
            "Console": "classes/",
            "Primo": "classes/",
            "Summon" : "classes/",
            "Pulfa" : "classes/",
            "FAQ" : "classes/",
            "Guides" : "classes/",
            "Utilities": "classes/",
            "Voyager" : "classes/",
            "Sitemap" : "classes/",
            "Springshare": "classes/",
            "Hours": "classes/",
            "Blacklight": "classes/",
            "Arts": "classes/"
          }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.3.*"
        }
    }
}
